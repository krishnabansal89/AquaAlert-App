{
  "expo": {
    "name": "AquaAlert",
    "slug": "AquaAlert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/nemo.png",
    "scheme": "aquaalert",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icons/nemo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/nemo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.krishna_bansal.AquaAlert"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "05deca66-f0a2-4290-84fa-62fbf740deb6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/05deca66-f0a2-4290-84fa-62fbf740deb6"
    }
  }
}
